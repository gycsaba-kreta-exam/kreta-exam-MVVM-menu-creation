<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:headerviews="clr-namespace:KretaDesktop.View.Header"
                    xmlns:configurationview="clr-namespace:KretaDesktop.View.Configuration"
                    xmlns:headerviewmodels="clr-namespace:KretaDesktop.ViewModel.Header"
                    xmlns:configurationviewmodels="clr-namespace:KretaDesktop.ViewModel.Configuration"
                    >

    <DataTemplate DataType="{x:Type headerviewmodels:ConfigurationHeaderViewModel}">
        <headerviews:ConfigurationHeader></headerviews:ConfigurationHeader>
    </DataTemplate>

    <DataTemplate DataType="{x:Type configurationviewmodels:LocalizationViewModel}">
        <configurationview:LocalizationView></configurationview:LocalizationView>
    </DataTemplate>
    
    
</ResourceDictionary>